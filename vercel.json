{
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }
  ],
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": { "maxLambdaSize": "15mb" }
    }
  ],
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ],
  "env": {
    "CI": "false"

  
  }
}
